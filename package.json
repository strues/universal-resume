{
  "name": "universal-resume",
  "version": "0.1.0",
  "description": "An over-engineered resume site.",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=7"
  },
  "scripts": {
    "analyze:client": "babel-node internal/scripts/analyze --client",
    "analyze:server": "babel-node internal/scripts/analyze --server",
    "build": "babel-node internal/scripts/build --optimize",
    "build:dev": "babel-node internal/scripts/build",
    "clean": "babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "develop": "babel-node internal/development",
    "lint": "eslint client server shared config internal",
    "preinstall": "node internal/scripts/preinstall",
    "start": "node build/server",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strues/universal-resume.git"
  },
  "author": "Steven Truesdell <steven@strues.io>",
  "keywords": [
    "react",
    "resume",
    "graphql",
    "universal",
    "javascript",
    "express",
    "apollo",
    "webpack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strues/universal-resume/issues"
  },
  "homepage": "https://github.com/strues/universal-resume#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ]
  },
  "dependencies": {
    "apollo-client": "^0.8.7",
    "app-root-dir": "1.0.2",
    "body-parser": "^1.16.1",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "dotenv": "4.0.0",
    "express": "4.14.1",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^1.2.4",
    "graphql-tools": "^0.10.0",
    "helmet": "3.4.0",
    "hpp": "0.2.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "normalize.css": "5.0.0",
    "offline-plugin": "4.6.1",
    "react": "15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-apollo": "^0.11.2",
    "react-async-component": "0.2.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-redux": "^5.0.2",
    "react-router-dom": "4.0.0-beta.6",
    "reactstrap": "^4.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0",
    "styled-components": "^1.4.3",
    "subscriptions-transport-ws": "^0.5.1",
    "uuid": "3.0.1",
    "webfontloader": "^1.6.27",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.3.2",
    "babel-plugin-dynamic-import-node": "^1.0.0",
